        "model.svm <- svm(formula = output ~.,",
        "+ ",
        "                 data = train_data)",
        "> ",
        "model.svm",
        "\nCall:\nsvm(formula = output ~ ., data = train_data)\n\n\nParameters:\n   SVM-Type:  C-classification \n SVM-Kernel:  radial \n       cost:  1 \n\nNumber of Support Vectors:  124\n\n",
        "> ",
        "",
        "> ",
        "## 5. Evaluation",
        "> ",
        "",
        "> ",
        "## Actual Value",
        "> ",
        "actual <- test_data$output",
        "> ",
        "",
        "> ",
        "#1. predicted Output - Logistic Regression",
        "> ",
        "pred.prob <- predict(logit, test_data, type=\"response\")",
        "> ",
        "pred.prob > 0.5",
        "    2     3     4     5     6    10    11    12    18    21    25    32    34 \nFALSE FALSE FALSE FALSE FALSE FALSE  TRUE FALSE FALSE  TRUE FALSE FALSE  TRUE \n   35    36    40    42    51    52    55    56    59    61    62    63    66 \nFALSE FALSE FALSE FALSE FALSE  TRUE FALSE FALSE FALSE FALSE FALSE FALSE FALSE \n   74    86    87    91    96    97    99   100   104   105   111   116   119 \n TRUE  TRUE FALSE FALSE  TRUE  TRUE FALSE FALSE FALSE FALSE  TRUE FALSE FALSE \n  122   124   131   135   140   144   145 ",
        "  156   163   167   168   180   182 \n TRUE FALSE FALSE FALSE  TRUE FALSE FALSE FALSE FALSE  TRUE  TRUE  TRUE  TRUE \n  183   184   185   190   193   194   203   204   206   212   216   217   219 \nFALSE  TRUE  TRUE FALSE  TRUE  TRUE  TRUE  TRUE  TRUE  TRUE  TRUE FALSE  TRUE \n  225   228   233   234   235   238   241   253   256   259   261   265   268 \n TRUE  TRUE  TRUE  TRUE  TRUE  TRUE  TRUE  TRUE  TRUE FALSE  TRUE  TRUE FALSE \n  270   272   275   279   281   287   290   294   296   298   299   300   303 \n TRUE ",
        "FALSE  TRUE FALSE  TRUE FALSE  TRUE  TRUE  TRUE  TRUE  TRUE FALSE FALSE \n",
        "> ",
        "pred.logit <- factor(pred.prob > 0.5,",
        "+ ",
        "                     levels = c(FALSE,TRUE),",
        "+ ",
        "                     labels = c(\"L\",\"M\"))",
        "> ",
        "",
        "> ",
        "#2. predicted Output - Decision Tree",
        "> ",
        "pred.dt <- predict(dt, test_data)",
        "> ",
        "",
        "> ",
        "#3. prediction Output - Random Forest",
        "> ",
        "pred.rf <- predict(rf, test_data)",
        "> ",
        "",
        "> ",
        "#4. prediction Output - SVM",
        "> ",
        "pred.svm <- predict(model.svm, test_data)",
        "> ",
        "",
        "> ",
        "## performance evaluation",
        "> ",
        "performance <- function(prediction, actual, method){",
        "+ ",
        "  #1 Create Confusion Matrix (cm)",
        "+ ",
        "  cm <- table(actual, prediction,",
        "+ ",
        "              dnn = c(\"actual\",\"predicted\"))",
        "+ ",
        "  ",
        "+ ",
        "  #2. extract TP, TN, FP, FN",
        "+ ",
        "  TP <- cm[2,2]",
        "+ ",
        "  TN <- cm[1,1]",
        "+ ",
        "  FP <- cm[1,2]",
        "+ ",
        "  FN <- cm[2,1]",
        "+ ",
        "  ",
        "+ ",
        "  #3. compute accuracy, precision, recall, f1score",
        "+ ",
        "  accuracy <- (TP+TN) / (TP+TN+FP+FN)",
        "+ ",
        "  precision <- TP/(FP+TP)",
        "+ ",
        "  recall <- TP/(FN+TP)",
        "+ ",
        "  f1score <- (2*precision*recall)/(precision+recall)",
        "+ ",
        "  ",
        "+ ",
        "  #4. prepare output function",
        "+ ",
        "  result <-paste(\"***Method: \", method,",
        "+ ",
        "                 \"\\n Accuracy = \", round(accuracy,3),",
        "+ ",
        "                 \"\\n Precision = \", round(precision,3),",
        "+ ",
        "                 \"\\n Recall = \", round(recall,3),",
        "+ ",
        "                 \"\\n F1 Score = \", round(f1score,3))",
        "+ ",
        "  cat (result)",
        "+ ",
        "}",
        "> ",
        "",
        "> ",
        "performance(pred.logit, actual, \"Logistic Regression\")",
        "***Method:  Logistic Regression \n Accuracy =  0.769 \n Precision =  0.75 \n Recall =  0.767 \n F1 Score =  0.759",
        "> ",
        "performance(pred.dt, actual, \"decision tree\")",
        "***Method:  decision tree \n Accuracy =  0.78 \n Precision =  0.829 \n Recall =  0.674 \n F1 Score =  0.744",
        "> ",
        "performance(pred.rf, actual, \"random forest\")",
        "***Method:  random forest \n Accuracy =  0.813 \n Precision =  0.825 \n Recall =  0.767 \n F1 Score =  0.795",
        "> ",
        "performance(pred.svm, actual, \"Support vactor Machine\")",
        "***Method:  Support vactor Machine \n Accuracy =  0.813 \n Precision =  0.795 \n Recall =  0.814 \n F1 Score =  0.805",
        "> ",
        "",
        "> ",
        "##6. recomendation",
        "> ",
        "install.packages(\"ggplot\")",
        "WARNING: Rtools is required to build R packages but is not currently installed. Please download and install the appropriate version of Rtools before proceeding:\n\nhttps://cran.rstudio.com/bin/windows/Rtools/\n",
